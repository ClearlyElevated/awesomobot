{
  "name": "awesomo-web",
  "version": "4.0.0",
  "description": "Website and dashboard for AWESOM-O",
  "main": "./src/app.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "dev": "cross-env START_HOT=1 webpack-dev-server --config webpack.dev.config.js",
    "prestart": "yarn build",
    "lint": "eslint src/**/*.jsx",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "yarn build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/test/.+\\.spec)\\.jsx?$",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:feinwarusoftware/awesomobot.git"
  },
  "author": {
    "name": "Feinwaru Software",
    "email": "feinwaru@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/feinwarusoftware/awesomobot/issues"
  },
  "homepage": "https://awesomo.feinwaru.com",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.8.8",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-meta-tags": "^0.7.4",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "universal-cookie": "^4.0.0"
  }
}
